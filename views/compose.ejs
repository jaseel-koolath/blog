<%- include("partials/header"); -%>
    <h1>Compose</h1>
    <form action="/compose" method="post">
        <div class="form-group">
            <label for="title"><h4>Title</h4></label>
            <input type="text" id="title" name="postTitle" class="form-control" autofocus>
            <label for="content"><h4>Content</h4></label>
            <textarea id="content" name="postBody" rows="5" class="form-control"></textarea>   
        </div>
        <button class="btn btn-primary" name="button" type="submit">Publish</button>
    </form>
    

<%- include("partials/footer"); -%>